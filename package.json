{
  "name": "e-controle",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build-all": "npm run build-questionnaire-list && npm run build-questionnaire-create && npm run build-questionnaire-detail ",
    "build-questionnaire-create": "parcel build static/src/questionnaire-create.js --out-dir ./static/dist/ --out-file questionnaire-create-bundle.js",
    "build-questionnaire-detail": "parcel build static/src/questionnaire-detail.js --out-dir ./static/dist/ --out-file questionnaire-detail-bundle.js",
    "build-questionnaire-list": "parcel build static/src/questionnaire-list.js --out-dir ./static/dist/ --out-file questionnaire-list-bundle.js",
    "watch-questionnaire-create": "parcel watch static/src/questionnaire-create.js --out-dir ./static/dist/ --out-file questionnaire-create-bundle.js",
    "watch-questionnaire-detail": "parcel watch static/src/questionnaire-detail.js --out-dir ./static/dist/ --out-file questionnaire-detail-bundle.js",
    "watch-questionnaire-list": "parcel watch static/src/questionnaire-list.js --out-dir ./static/dist/ --out-file questionnaire-list-bundle.js",
    "postinstall": "npm run build-all"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "dist": "^0.1.2",
    "dropzone": "^5.5.1",
    "moment": "^2.24.0",
    "store": "^2.0.12",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-hot-reload-api": "^2.3.3",
    "vue2-dropzone": "^3.5.9",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.1.1",
    "vuex-map-fields": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-stage-2": "^7.0.0",
    "@vue/component-compiler-utils": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.5.1",
    "vue-template-compiler": "^2.6.10"
  }
}
